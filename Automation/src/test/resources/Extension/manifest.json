{
  "version": "1.0.0",
  "manifest_version": 2,
  "name": "Proxy Authentication",
  "permissions": [
    "proxy",
    "tabs",
    "unlimitedStorage",
    "storage",
    "<all_urls>", // Changed to apply to all URLs
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "minimum_chrome_version": "22.0.0"
}